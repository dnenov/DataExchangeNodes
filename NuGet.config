<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- Clear any inherited sources to use only what we specify -->
    <clear />
    <!-- Autodesk Artifactory - contains Autodesk.DataExchange.UI and other Autodesk packages -->
    <add key="Autodesk Artifactory" value="https://art-bobcat.autodesk.com/artifactory/api/nuget/autodesk-3p-nuget/" />
    <!-- Dynamo Artifactory - contains Dynamo packages -->
    <add key="Dynamo Artifactory" value="https://art-bobcat.autodesk.com/artifactory/api/nuget/team-dynamo-nuget" />
    <!-- Public NuGet feed -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  <disabledPackageSources>
    <!-- Disable Microsoft and .NET feeds to avoid conflicts -->
    <add key="Microsoft and .NET" value="true" />
  </disabledPackageSources>
  <activePackageSource>
    <!-- Use aggregate source to search all enabled sources -->
    <add key="All" value="(Aggregate source)" />
  </activePackageSource>
  <!-- Increase timeout for slow VPN connections -->
  <config>
    <add key="http_proxy" value="" />
    <add key="signatureValidationMode" value="accept" />
  </config>
  <!-- Package source credentials and timeout settings -->
  <packageSourceCredentials />
</configuration>
